# Firebase Configuration
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id

# Google Maps API Configuration (Task 2)
# Get your API key from: https://console.cloud.google.com/google/maps-apis/overview
GOOGLE_MAPS_API_KEY=your_google_maps_directions_api_key
# Daily budget limit for Google Maps API costs (default: $10)
GOOGLE_MAPS_DAILY_BUDGET=10
# Rate limit for Google Maps API (requests per second, default: 50)
GOOGLE_MAPS_RATE_LIMIT=50

# OneMap Singapore API (FREE fallback)
# Register at: https://www.onemap.gov.sg/docs/
ONEMAP_EMAIL=your_onemap_email
ONEMAP_PASSWORD=your_onemap_password

# MapBox API
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_access_token
EXPO_PUBLIC_MAPBOX_DOWNLOAD_TOKEN=your_mapbox_download_token

# Google Places API (minimal usage for venue discovery - Task 4)
EXPO_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Environment
EXPO_PUBLIC_ENVIRONMENT=development

# Cache Configuration
# Cache duration for travel times (minutes)
TRAVEL_TIME_CACHE_DURATION=30
# Cache duration for directions during peak hours (minutes)
PEAK_HOUR_CACHE_DURATION=15
# Maximum cache entries
MAX_CACHE_ENTRIES=1000