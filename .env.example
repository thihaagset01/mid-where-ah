# MidWhereAh Environment Variables Example
# IMPORTANT: Copy this file to .env and replace placeholder values with your actual credentials
# DO NOT commit your actual credentials to version control

# Flask Configuration
FLASK_SECRET_KEY=your_secure_random_secret_key_here
FLASK_DEBUG=True

# Google Maps API
# Get from: https://console.cloud.google.com/google/maps-apis/credentials
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_DISTANCE_MATRIX_QUOTA_DAILY=2500
MAX_CONCURRENT_API_REQUESTS=10

# Firebase Configuration (JSON format)
# Get from: Firebase Console > Project Settings > General > Your Apps > Web App > Firebase SDK snippet
FIREBASE_CONFIG={"apiKey":"your_api_key","authDomain":"your_project_id.firebaseapp.com","projectId":"your_project_id","storageBucket":"your_project_id.appspot.com","messagingSenderId":"your_messaging_sender_id","appId":"your_app_id","measurementId":"your_measurement_id"}
