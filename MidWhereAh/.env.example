# Environment Variables for MidWhereAh - Example

# Copy this file to .env.local and fill in your actual values
# Never commit actual API keys to version control

# Development/Production
NODE_ENV=development

# API Keys (Get from respective services)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
EXPO_PUBLIC_ONEMAP_API_KEY=your_onemap_api_key_here

# Firebase Configuration (Get from Firebase Console)
EXPO_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key_here
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
EXPO_PUBLIC_FIREBASE_PROJECT_ID=your_project_id_here
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
EXPO_PUBLIC_FIREBASE_APP_ID=your_app_id_here

# App Configuration
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_API_BASE_URL=https://api.midwhereah.com

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false
EXPO_PUBLIC_DEBUG_MODE=true

# Performance Monitoring
EXPO_PUBLIC_PERFORMANCE_MONITORING=false

# Accessibility
EXPO_PUBLIC_ACCESSIBILITY_TESTING=true